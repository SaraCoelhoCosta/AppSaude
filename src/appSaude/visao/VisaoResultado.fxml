<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane xmlns="http://javafx.com/javafx/16"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="appSaude.controle.ControleResultado">
	<columnConstraints>
		<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
	</columnConstraints>
	<rowConstraints>
		<RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
	</rowConstraints>
	<children>
		<Pane fx:id="painelResultado" maxHeight="-Infinity"
			maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
			prefHeight="250.0" prefWidth="320.0" styleClass="geral"
			stylesheets="@Aparencia.css">
			<children>
				<Label fx:id="labelTitulo" layoutY="23.0" prefHeight="40.0"
					prefWidth="320.0" styleClass="titulo" text="Resultado" />
				<Label fx:id="labelCalculo" alignment="CENTER" layoutX="50.0"
					layoutY="77.0" prefHeight="87.0" prefWidth="228.0"
					styleClass="label" text="Calculo" textAlignment="CENTER" />
				<Button fx:id="botaoConfirmar" layoutX="108.0"
					layoutY="179.0" mnemonicParsing="false" onAction="#confirmar"
					styleClass="botao" text="Confirmar" />
			</children>
		</Pane>
	</children>
</GridPane>
